{% extends 'menu.base.html.twig' %}

{% block title %}Agency{% endblock %}

{% block body %}
    <h1>Agency</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ agency.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ agency.name }}</td>
            </tr>
            <tr>
                <th>Surname</th>
                <td>{{ agency.surname }}</td>
            </tr>
            <tr>
                <th>Agency</th>
                <td>{{ agency.agency }}</td>
            </tr>
            <tr>
                <th>Website</th>
                <td>{{ agency.website }}</td>
            </tr>
            <tr>
                <th>Country</th>
                <td>{{ agency.country }}</td>
            </tr>
            <tr>
                <th>Email</th>
                <td>{{ agency.email }}</td>
            </tr>
            <tr>
                <th>Password</th>
                <td>{{ agency.password }}</td>
            </tr>
            <tr>
                <th>Role</th>
                <td>{{ agency.role }}</td>
            </tr>


        </tbody>
    </table>

    <a href="{{ path('agency_index') }}">back to list</a>

    <a href="{{ path('agency_edit', {'id': agency.id}) }}">edit</a>

    {{ include('agency/_delete_form.html.twig') }}
